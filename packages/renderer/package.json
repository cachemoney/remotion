	},
	"bugs": {
		"url": "https://github.com/remotion-dev/remotion/issues"
	},
	"dependencies": {
		"execa": "5.1.1",
		"extract-zip": "2.0.1",
		"remotion": "workspace:*",
		"source-map": "^0.8.0-beta.0",
		"ws": "8.7.0"
	},
	"peerDependencies": {
		"react": ">=16.8.0",
		"react-dom": ">=16.8.0"
	},
	"devDependencies": {
		"@jonny/eslint-config": "3.0.266",
		"@testing-library/dom": "^8.16.0",
		"@testing-library/react": "13.3.0",
		"@types/node": "^16.7.5",
		"@types/progress": "2.0.5",
