{
	"name": "remotion",
	"version": "3.3.54",
	"description": "Render videos in React",
	"main": "dist/cjs/index.js",
	"types": "dist/cjs/index.d.ts",
	"module": "dist/esm/index.js",
	"scripts": {
		"prepublishOnly": "node ensure-correct-version.js && cp ../../README.md .",
		"lint": "eslint src --ext ts,tsx",
		"test": "vitest --run",
		"build": "tsc -d && tsc -d -p tsconfig-cjs.json",
		"watch": "tsc -w"
	},
	"repository": {
		"url": "https://github.com/remotion-dev/remotion"
	},
	"bugs": {
		"url": "https://github.com/remotion-dev/remotion/issues"
	},
	"author": "",
	"license": "SEE LICENSE IN LICENSE.md",
	"peerDependencies": {
		"react": ">=16.8.0",
		"react-dom": ">=16.8.0"
	},
	"devDependencies": {
		"@jonny/eslint-config": "3.0.266",
		"@testing-library/react": "14.0.0",
		"@types/node": "^16.7.5",
		"@types/react": "18.0.26",
		"@types/react-dom": "18.0.10",
		"@types/webpack-env": "^1.16.0",
		"@vitejs/plugin-react": "^2.0.0",
		"eslint": "8.25.0",
		"eslint-plugin-require-extensions": "^0.1.2",
		"jsdom": "21.1.0",
		"prettier": "^2.7.1",
		"prettier-plugin-organize-imports": "^2.3.4",
		"react": "^18.0.0",
		"react-dom": "^18.0.0",
		"rimraf": "^3.0.2",
		"typescript": "^4.7.0",
		"vitest": "0.24.3"
	},
	"keywords": [
		"remotion",
		"ffmpeg",
		"video",
		"react",
		"player"
	],
	"publishConfig": {
		"access": "public"
	},
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./dist/cjs/index.d.ts",
			"module": "./dist/esm/index.js",
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		},
		"./version": {
			"types": "./dist/cjs/version.d.ts",
			"module": "./dist/esm/version.js",
			"import": "./dist/esm/version.js",
			"require": "./dist/cjs/version.js"
		}
	},
	"typesVersions": {
		">=1.0": {
			"version": [
				"dist/esm/version.d.ts"
			]
		}
	}
}
