{
	"name": "@remotion/player",
	"version": "3.3.84",
	"description": "Remotion Player",
	"main": "dist/cjs/index.js",
	"types": "dist/cjs/index.d.ts",
	"module": "dist/esm/index.mjs",
	"sideEffects": false,
	"scripts": {
		"lint": "eslint src --ext ts,tsx",
		"build": "node ensure-correct-version.js && rollup --config rollup.config.js && tsc -d",
		"watch": "tsc -w",
		"prerelease": "cp ../../README.md ."
	},
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./dist/cjs/index.d.ts",
			"module": "./dist/esm/index.mjs",
			"import": "./dist/esm/index.mjs",
			"require": "./dist/cjs/index.js"
		}
	},
	"repository": {
		"url": "https://github.com/remotion-dev/remotion"
	},
	"bugs": {
		"url": "https://github.com/remotion-dev/remotion/issues"
	},
	"files": [
		"dist",
		"README.md"
	],
	"author": "Jonny Burger <jonny@remotion.dev>",
	"maintainers": [
		"Jonny Burger <jonny@remotion.dev>",
		"Shankhadeep Dey <shankhadeepdey99@gmail.com>"
	],
	"license": "SEE LICENSE IN LICENSE.md",
	"dependencies": {
		"remotion": "workspace:*"
	},
	"peerDependencies": {
		"react": ">=16.8.0",
		"react-dom": ">=16.8.0"
	},
	"devDependencies": {
		"@jonny/eslint-config": "3.0.273",
		"@rollup/plugin-typescript": "^8.2.0",
		"@testing-library/react": "13.1.1",
		"@types/node": "^14.14.14",
		"@types/react": "18.0.26",
		"@types/react-dom": "18.0.10",
		"@vitejs/plugin-react": "^2.0.0",
		"csstype": "^3.1.1",
		"eslint": "8.25.0",
		"eslint-plugin-require-extensions": "^0.1.2",
		"jsdom": "20.0.1",
		"prettier": "^2.7.1",
		"prettier-plugin-organize-imports": "^2.3.4",
		"react": "^18.0.0",
		"react-dom": "^18.0.0",
		"rollup": "^2.70.1",
		"typescript": "^4.7.0",
		"vitest": "0.24.3",
		"webpack": "5.76.1"
	},
	"keywords": [
		"remotion",
		"ffmpeg",
		"video",
		"react",
		"player"
	],
	"publishConfig": {
		"access": "public"
	}
}
