		"eslint": "8.25.0",
		"prettier": "^2.7.1",
		"prettier-plugin-organize-imports": "^2.3.4",
		"react": "18.0.0",
		"react-dom": "18.0.0",
		"remotion": "workspace:*",
		"rollup": "^2.70.1",
